import org.bukkit.ChatColor;

public enum Team {

    DEMON(ChatColor.BLUE + "青鬼"),
    FUGITIVE(ChatColor.GOLD + "逃走"),
    JAIL(ChatColor.BLACK + "牢屋"),
    ADMIN(ChatColor.GREEN + "運営");

    private String team;

    Team(String team) {
        this.team = team;
    }

    public String getStatus() {
        return team;
    }

}
